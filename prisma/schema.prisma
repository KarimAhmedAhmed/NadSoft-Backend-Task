
generator client {
  provider = "prisma-client-js"
  output   = "./prisma/client"
}

datasource db {
  provider = "postgresql"
  url ="postgresql://postgres:postgres@localhost:5432/postgres"
}


// Define User model with auto-incrementing id
model User {
  id      Int     @id @default(autoincrement())
  name    String 
  email   String  @unique
  age     Int
  country String
  mobile  String  @unique
}
